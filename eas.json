{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.12.0",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_NPM_CACHE_URL": "false"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "22.12.0",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_NPM_CACHE_URL": "false"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "node": "22.12.0",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}